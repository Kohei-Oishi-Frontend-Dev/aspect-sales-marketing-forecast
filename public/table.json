  "query_info": {
    "sql": "WITH actual_sales AS ( SELECT EXTRACT(MONTH FROM date) AS month, SUM(sale) AS actual_sales FROM `aspect-business-management.sales_data.raw_sales_data` WHERE date <= '2024-08-31' GROUP BY month ), forecasted_sales AS ( SELECT EXTRACT(MONTH FROM date) AS month, SUM(pred_sales) AS forecasted_sales FROM `aspect-business-management.sales_data.predicted_sales_data` WHERE date > '2024-08-31' GROUP BY month ) SELECT COALESCE(actual_sales.month, forecasted_sales.month) AS month, actual_sales.actual_sales, forecasted_sales.forecasted_sales FROM actual_sales FULL OUTER JOIN forecasted_sales ON actual_sales.month = forecasted_sales.month ORDER BY month LIMIT 10000",
